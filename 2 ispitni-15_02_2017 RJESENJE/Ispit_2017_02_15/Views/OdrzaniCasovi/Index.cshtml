
@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

@using Ispit_2017_02_15.ViewModels
@model OdrzaniCasovi_IndexVM


<table class="table table-striped">
    <thead>
        <tr>
            <th>ime prezime</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var i in Model._lista)
        {
            <tr>
                <td>@i.imePrezime</td>
                <td>
                    <a class="btn btn-primary" href="/OdrzaniCasovi/DetaljiCasova?nastavnikID=@i.nastavnikID">Detalji</a>
                </td>
            </tr>
        }
    </tbody>
</table>